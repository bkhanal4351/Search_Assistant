{
  "permissions": {
    "allow": [
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git push:*)",
      "Bash(while read hash msg)",
      "Bash(do git log -1 --format=\"%b\" \"$hash\")",
      "Bash(echo:*)",
      "Bash(done)",
      "Bash(git filter-branch:*)",
      "Bash(while read ref)",
      "Bash(do git update-ref -d \"$ref\")"
    ]
  }
}
